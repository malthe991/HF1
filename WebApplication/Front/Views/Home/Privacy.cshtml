@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
@foreach (var item in BLL.BllClass.HentAltData())
{
    <tr>
        <td>@item.id</td>
        <td>@item.first_name</td>
        <td>@item.last_name</td>
        <td>@item.email</td>
        <td>@item.username</td>
    </tr>
}